{
  "name": "c24-website",
  "version": "1.0.0",
  "description": "Website of TUM Blockchain Club",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:f": "npm --workspace frontend run dev",
    "build:f": "npm --workspace frontend run build",
    "start:f": "npm --workspace frontend run start",
    "lint:f": "npm --workspace frontend run lint",
    "build:b": "npm --workspace strapi run build",
    "deploy:b": "npm --workspace strapi run deploy",
    "dev:b": "npm --workspace strapi run develop",
    "start:b": "npm --workspace strapi run start",
    "strapi": "npm --workspace strapi run strapi",
    "dev": "npm-run-all --parallel dev:f dev:b",
    "build": "npm-run-all --parallel build:f build:b"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TUM-Blockchain-Club/website.git"
  },
  "keywords": [
    "next",
    "strapi",
    "blockchain"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TUM-Blockchain-Club/website/issues"
  },
  "homepage": "https://github.com/TUM-Blockchain-Club/website#readme",
  "workspaces": [
    "frontend",
    "strapi"
  ],
  "devDependencies": {
    "eslint": "^9",
    "eslint-config-prettier": "^9.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "3.4.2"
  }
}
